    "commander": "^2.11.0",
    "crypto-js": "^3.1.9-1",
    "elliptic": "^6.4.0",
    "merkle": "^0.5.1",
    "secp256k1": "^3.3.0",
    "socket.io": "^2.0.3",
    "socket.io-client": "^2.0.3",
    "ts-node": "^3.3.0",
